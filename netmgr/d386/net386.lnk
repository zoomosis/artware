system DOS4G
name ..\net386.exe
option eliminate
option cache
option stack=32k
option map
option description 'NetMgr, netmail manager for JAM, Squish, *.MSG and Hudson'

debug all
option verbose

library \tc\msgapi\386api

file  netmgr.obj, register.obj, config.obj, wrap.obj,
      txtbuild.obj, version7.obj, fdnode.obj, gigo.obj, binkpack.obj,
      command.obj, xfile.obj, lastread.obj, akamatch.obj, external.obj,
      mem.obj
 
